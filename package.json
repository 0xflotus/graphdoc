{
  "name": "@2fd/graphdoc",
  "version": "1.2.0",
  "description": "Static page generator for documenting GraphQL Schema",
  "main": "bin/graphdoc",
  "bin": {
    "graphdoc": "./bin/graphdoc.js"
  },
  "scripts": {
    "compile": "tsc -p .",
    "declaration": "tsc -d -p tsconfig.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2fd/graphdoc.git"
  },
  "keywords": [
    "graphql",
    "documentation"
  ],
  "author": "Fede Ramirez <i@2fd.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2fd/graphdoc/issues"
  },
  "homepage": "https://github.com/2fd/graphdoc#readme",
  "devDependencies": {
    "@salesforce-ux/design-system": "^2.1.4",
    "@types/chai": "^3.4.34",
    "@types/fs-extra": "0.0.32",
    "@types/glob": "^5.0.30",
    "@types/graphql": "^0.8.6",
    "@types/marked": "0.0.27",
    "@types/mocha": "^2.2.32",
    "@types/mustache": "^0.8.28",
    "@types/node": "^6.0.32",
    "chai": "^3.5.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.0",
    "jest": "^18.1.0",
    "node-sass": "^3.8.0",
    "typescript": "2.0.0"
  },
  "dependencies": {
    "@2fd/command": "^1.1.2",
    "@types/jest": "^18.1.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.0",
    "marked": "^0.3.6",
    "mustache": "^2.2.1"
  },
  "graphdoc": {
    "endpoint": "http://localhost:4000/graphql",
    "output": "./gh-pages/star-wars",
    "force": true,
    "verbose": true
  }
}
