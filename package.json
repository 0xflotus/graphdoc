{
  "name": "@2fd/graphdoc",
  "version": "1.0.0",
  "description": "Static page generator for GraphQL.js",
  "main": "bin/graphdoc",
  "bin": {
    "graphdoc": "./bin/graphdoc.js"
  },
  "scripts": {
    "compile": "tsc -p .",
    "declaration": "tsc -d -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2fd/graphdoc.git"
  },
  "keywords": [
    "graphql",
    "documentation"
  ],
  "author": "Fede Ramirez <i@2fd.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2fd/graphdoc/issues"
  },
  "homepage": "https://github.com/2fd/graphdoc#readme",
  "devDependencies": {
    "@salesforce-ux/design-system": "^2.0.3",
    "@types/marked": "0.0.27",
    "@types/mustache": "^0.8.28",
    "@types/node": "^6.0.32",
    "chai": "^3.5.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.0",
    "node-sass": "^3.8.0",
    "typescript": "2.0.0"
  },
  "dependencies": {
    "@2fd/command": "^1.1.0",
    "fs.extra": "^1.3.2",
    "marked": "^0.3.6",
    "mustache": "^2.2.1"
  },
  "graphdoc": {
    "endpoint": "http://localhost:4000/graphql",
    "output": "./build",
    "force": true,
    "verbose": true
  }
}
