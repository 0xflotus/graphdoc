{{!
 Only navs varaible is defined:
 @example:
    navs: [
        {
            title: String,
            items: [
                {
                    href: String,
                    text: String
                }

                -- MORE ITEMS ---
            ]
        }

        -- MORE NAVS --
    ]
}}{{#navs}}
<div class="slds-tree_container" role="application">
    <h4 class="slds-text-heading--label slds-m-top--small" id="treeheading">{{title}}</h4>
    <ul class="slds-tree" role="tree" aria-labelledby="treeheading">
        {{#items}}
        <li id="tree0-node0" role="treeitem" aria-level="1">
            <div class="slds-tree__item"><a href="{{href}}" class="slds-truncate">{{text}}</a></div>
        </li>
        {{/items}}
    </ul>
</div>
{{/navs}}